{
  "name": "react-datamaps-india",
  "version": "0.5.0",
  "private": false,
  "description": "Datamaps for india region",
  "license": "MIT",
  "author": {
    "name": "Shyam Gupta",
    "email": "shyamm@outlook.com",
    "url": "https://sshyam-gupta@netlify.com"
  },
  "homepage": "https://react-datamaps.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sshyam-gupta/react-datamaps-india.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "yarn build:cjs && yarn build:esm && yarn build:umd && yarn build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanup",
    "package": "yarn build && yarn pack",
    "build:example": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "prettier": "prettier --ignore-path .gitignore --write './**/*.{js,jsx,ts,tsx,json}'"
  },
  "dependencies": {
    "d3-geo": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "topojson-client": "^3.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/d3-geo": "^3.0.2",
    "@types/d3-interpolate": "^3.0.1",
    "@types/d3-scale": "^4.0.1",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/topojson-client": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "install-peers-cli": "^2.2.0",
    "jest": "^27.2.0",
    "json-loader": "^0.5.7",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}